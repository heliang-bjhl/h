					<li>
						<input class="m-ip" type="date" placeholder="生日" data-v="require-date" value="2014-03-09" />
						<div class="m-err"></div>
					</li>
					<li>
						<input class="m-bt1" type="submit" value="提交" id="j_sb" />
					</li>
				</ul>
			</form>
		</div>
	</div>
</div>

